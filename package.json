{
  "name": "@peaks/umi-clone-file-gitlab",
  "version": "1.1.0",
  "description": "umijs框架的插件包，实现从其他仓拉取文件到当前仓库",
  "main": "src/index.ts",
  "scripts": {
    "test": "umi-test",
    "build": "rm -rf ./lib && tsc",
    "lint": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "release": "standard-version",
    "pack": "npm pack",
    "login": "npm login",
    "publish": "pnpm run release && npm publish --ignore-scripts"
  },
  "umi": {
    "plugins": [
      "./src/index.ts"
    ]
  },
  "keywords": [],
  "author": "zhuhaifeng",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@umijs/test": "^4.5.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "umi": "^4.5.3"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dayjs": "^1.11.18"
  }
}
